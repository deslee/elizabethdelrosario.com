{
  "name": "postgraphile-next-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all server:runts",
    "debug": "npm-run-all server:debug",
    "server:runts": "node -r ts-node/register server/index.ts",
    "server:debug": "cross-env DEBUG=\"postgraphile:graphql,postgraphile:request,postgraphile:postgres*\" node -r ts-node/register --inspect server/index.ts",
    "migration:create": "cross-env ts-node ./node_modules/typeorm/cli.js migration:create -n Migration -f database/typeOrmConfig.ts -d database/migrations/",
    "migration:generate": "cross-env ts-node ./node_modules/typeorm/cli.js migration:generate -n Migration -f database/typeOrmConfig.ts -d database/migrations/",
    "schema:sync": "cross-env ts-node ./node_modules/typeorm/cli.js schema:sync -f database/typeOrmConfig.ts",
    "migration:run": "cross-env ts-node ./node_modules/typeorm/cli.js migration:run -f database/typeOrmConfig.ts",
    "migration:revert": "cross-env ts-node ./node_modules/typeorm/cli.js migration:revert -f database/typeOrmConfig.ts",
    "embedded-schema:generate-typedef": "ts-node server/embeddedGraphql/generate-schema-script.ts",
    "embedded-schema:generate-binding": "graphql-binding --language typescript --input server/embeddedGraphql/generated/typedef.graphql --outputBinding server/embeddedGraphql/bindings/generated.ts && ts-node server/embeddedGraphql/patch-binding.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphile-contrib/pg-simplify-inflector": "^5.0.0-beta.1",
    "@zeit/next-typescript": "^1.1.1",
    "apollo-boost": "^0.4.0",
    "apollo-link-schema": "^1.2.2",
    "apollo-server-express": "^2.5.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "convict": "^5.0.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphile-utils": "^4.4.0",
    "graphql-binding": "^2.5.1",
    "graphql-import": "^0.7.1",
    "graphql-tools": "^4.0.4",
    "jsonwebtoken": "^8.5.1",
    "next": "^8.1.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "postgraphile": "^4.4.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.6",
    "react-dom": "^16.8.6",
    "typeorm": "^0.2.17",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "0.0.36",
    "@types/convict": "^4.2.1",
    "@types/cookie-parser": "^1.4.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/next": "^8.0.5",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/react": "^16.8.18",
    "@types/uuid": "^3.4.4",
    "cross-env": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.2.0",
    "ts-node-dev": "^1.0.0-pre.39",
    "typescript": "^3.4.5"
  }
}
