    "graphile-utils": "^4.4.0",
    "graphql-binding": "^2.5.1",
    "graphql-import": "^0.7.1",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.4",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "micro-compose": "0.0.3",
    "mime-types": "^2.1.24",
    "next": "^8.1.0",
    "notistack": "^0.8.5",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
