{
    "version": 2,
    "name": "elizabethdelrosariocom",
    "env": {
        "S3_UPLOAD_ACCESS_KEY_ID": "@s3_upload_access_key_id",
        "S3_UPLOAD_SECRET_ACCESS_KEY": "@s3_upload_secret_access_key",
        "S3_UPLOAD_BUCKET": "@s3_upload_bucket",
        "JWT_SECRET": "@jwt_secret",
        "DB_HOST": "dev.le3.io",
        "DB_NAME": "elizabethdelrosariocom",
        "DB_ADMIN_NAME": "postgres",
        "DB_ADMIN_PASS": "@db_admin_pass",
        "DB_USER_NAME": "beth_user",
        "DB_USER_PASS": "@db_user_pass"
    },
    "builds": [
        {
            "src": "now-build.sh",
            "use": "@now/static-build"
        },
        {
            "src": "api/graphql.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/graphql",
            "dest": "api/graphql.js"
        },
        {
            "src": "/admin/static/(.*)",
            "headers": {
                "cache-control": "s-maxage=31536000,immutable"
            },
            "dest": "admin/static/$1"
        },
        {
            "src": "/admin/favicon.ico",
            "dest": "admin/favicon.ico"
        },
        {
            "src": "/admin/asset-manifest.json",
            "dest": "admin/asset-manifest.json"
        },
        {
            "src": "/admin/manifest.json",
            "dest": "admin/manifest.json"
        },
        {
            "src": "/admin/precache-manifest.(.*)",
            "dest": "admin/precache-manifest.$1"
        },
        {
            "src": "/admin/service-worker.js",
            "headers": {
                "cache-control": "s-maxage=0"
            },
            "dest": "admin/service-worker.js"
        },
        {
            "src": "/admin/sockjs-node/(.*)",
            "dest": "admin/sockjs-node/$1"
        },
        {
            "src": "/admin/(.*)",
            "headers": {
                "cache-control": "s-maxage=0"
            },
            "dest": "admin/"
        }
    ]
}